#: ../properties/nm-openvpn-dialog.ui.h:35
msgid "Key Direction:"
msgstr "Усмерење кључа:"

#: ../properties/nm-openvpn-dialog.ui.h:36
msgid "Static Key:"
msgstr "Статички кључ:"

#: ../properties/nm-openvpn-dialog.ui.h:37
