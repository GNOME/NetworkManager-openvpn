msgstr "SHA-512"

#: properties/nm-openvpn-editor.c:1022
msgid "RIPEMD-160"
msgstr "RIPEMD-160"

#: properties/nm-openvpn-editor.c:1080
msgid "Don’t verify certificate identification"
msgstr "Ez egiaztatu ziurtagiriaren identifikazioa"

#: properties/nm-openvpn-editor.c:1086
msgid "Verify whole subject exactly"
msgstr "Egiaztatu gai osoa zehazki"

#: properties/nm-openvpn-editor.c:1092
msgid "Verify name exactly"
msgstr "Egiaztatu izena zehazki"

